<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#20534e" />
            <gradient  android:angle="45"  android:centerX="25%" android:startColor="#062d30" android:endColor="#43797d"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#20534e" />
            <solid  android:color="#125156"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#20534e" />
            <gradient  android:angle="45"  android:startColor="#43797d" android:endColor="#125156" />
        </shape>
    </item>
</selector>